{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://hubflow.store/:path*"
    },
    { "source": "/(.*)", "destination": "/" },
    {
      "source": "/user/thumb/:path*",
      "destination": "https://hubflow.store/user/thumb/:path*",
      "statusCode": 308
    },
    {
      "source": "/message/img/:path*",
      "destination": "https://hubflow.store/message/img/:path*",
      "statusCode": 308
    }
  ]
}
